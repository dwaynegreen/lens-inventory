@page

@model AdminPages.AdminIndexModel
@{
    ViewData["Title"] = "Admin";
}

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                Materials
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Materials)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelIem => item.Name)
                </td>
                <td>
                    <a asp-page="./Delete" asp-route-id="@item.Id">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>

